{
  "name": "Vieb",
  "version": "0.1.0",
  "description": "Vim Inspired Electron Browser",
  "main": "app/index.js",
  "build": {
    "appId": "com.github.VimprovedVenture.Vieb",
    "linux": {
      "executableName": "Vieb",
      "icon": "build/icons",
      "category": "Network;WebBrowser;",
      "maintainer": "Vimproved Venture",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "icon": "build/icons",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "build/icons",
      "legalTrademarks": "Copyright @ Vimproved Venture | Licensed as free software (GPL-3.0+)",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "license": "LICENSE"
    },
    "fileAssociations": [
      {
        "ext": "html"
      }
    ],
    "protocols": [
      {
        "name": "Websites",
        "schemes": [
          "http",
          "https"
        ]
      }
    ],
    "copyright": "Copyright @ Vimproved Venture | Licensed as free software (GPL-3.0+)",
    "extraFiles": [
      "README.md",
      "LICENSE"
    ]
  },
  "scripts": {
    "start": "electron app",
    "buildall": "electron-builder --linux --mac --win",
    "buildlinux": "electron-builder --linux",
    "buildmac": "electron-builder --mac",
    "buildwin": "electron-builder --win"
  },
  "repository": "https://github.com/VimprovedVenture/Vieb",
  "homepage": "https://github.com/VimprovedVenture/Vieb",
  "keywords": [
    "Electron",
    "Browser",
    "Internet"
  ],
  "author": "Vimproved Venture",
  "license": "GPL-3.0+",
  "devDependencies": {
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "eslint": "^5.12.1"
  }
}
